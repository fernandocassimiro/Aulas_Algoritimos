// Seção de Declarações 
var
X, Y, Z : inteiro
inicio
      escreva ("digite o primeiro numero: ")
      leia (x)
      escreva ("digite o segundo numero: ")
      leia (y)
z <- (x+y)
escreva ("resultado e: ", z)
fimalgoritmo
